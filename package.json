    "@vue/compiler-sfc": "3.0.11",
    "@vue/eslint-config-standard": "6.0.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.18.0",
    "bootstrap-vue": "2.21.2",
    "bootstrap": "4.6.0",
    "core-js": "3.8.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "5.0.0",
    "eslint-plugin-vue": "7.5.0",
